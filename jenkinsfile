pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
    

        stage('Deploy') {
            steps {
                echo 'Deploymnet'
            }
        }
    
    
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
    
  
        stage('Release') {
            steps {
                echo 'PROD Release'
            }
        }

        
    }
    
}
